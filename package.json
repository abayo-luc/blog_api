{
  "name": "new-nodejs-app",
  "version": "1.0.0",
  "description": "NodeJs REST API",
  "main": "app.js",
  "scripts": {
    "test": "jest --collect-coverage",
    "dev": "NODE_ENV=development nodemon --exec babel-node ./src/app.js",
    "clean:dist": "rm -rf dist && mkdir dist",
    "build-server": "babel -d ./dist ./src",
    "build": "npm run clean:dist && npm run build-server",
    "start": "node ./dist/app.js",
    "db:migrate": "node_modules/.bin/sequelize db:migrate",
    "db:migrate:seed": "node_modules/.bin/sequelize db:seed:all"
  },
  "keywords": [
    "NodeJS",
    "generate-node-project"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "@hapi/joi": "^17.1.1",
    "admin-bro": "2.0.0",
    "admin-bro-expressjs": "^2.0.0",
    "admin-bro-sequelizejs": "^0.4.0",
    "admin-bro-theme-dark": "^1.0.0",
    "bcrypt": "^4.0.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-formidable": "^1.2.0",
    "express-ip": "^1.0.4",
    "express-session": "^1.17.0",
    "lodash": "^4.17.21",
    "moment": "^2.24.0",
    "pg": "^7.18.2",
    "pg-hstore": "^2.3.3",
    "react": "^16.13.1",
    "recharts": "^1.8.5",
    "sequelize": "^4.44.4",
    "sequelize-cli": "^5.5.1",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.9.4",
    "@babel/register": "^7.7.0",
    "babel-jest": "^24.8.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "3.1.1",
    "faker": "^4.1.0",
    "jest": "^24.8.0",
    "nodemon": "^2.0.0",
    "supertest": "^4.0.2"
  }
}
